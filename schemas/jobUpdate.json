{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "title"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "company handle"
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                1.0
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                10000
            ]
        }
    },
    "examples": [{
        "title": "title",
        "companyHandle": "company handle",
        "equity": 1.0,
        "salary": 10000
    }]
}